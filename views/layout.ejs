  <div class="wrapper row1">
    <header id="header" class="hoc clear">
      <div id="logo" class="fl_left">
        <!-- ################################################################################################ -->
        <h1><a href="/">uBay</a></h1>
        <!-- ################################################################################################ -->
      </div>
      <nav id="mainav" class="fl_right">
        <!-- ################################################################################################ -->
        <ul class="clear">
          <li><a href="/">Home</a></li>
          <li><a class="drop" href="#">Categories</a>
            <ul>
              <li><a href="/category/Shoes">Shoes</a></li>
              <li><a href="/category/Electronics">Electronics</a></li>
              <li><a href="/category/Clothing">Clothing</a></li>
              <li><a href="/category/Books">Books</a></li>
              <li><a href="/category/Other">Others</a></li>
            </ul>
          </li>


          <% if (loggedIn && currentUser.isAdmin) { %>
          <li><a href="">logged in as<%= currentUser.username %></a>
            <ul>
              <li><a href="/">Approve Products</a></li>
              <li><a href="/admin">Approve BID</a></li>
              <li><a href="<%=`/users/${currentUser._id}`%>">Admin profile</a></li>
            </ul>
          </li>
          <li><a href="/users/logout">Log out</a></li>
          <%} else {      %>

          <% if (loggedIn && currentUser.isAdmin===false) {  %>
          <li><a href=""> Logged in as <%= currentUser.username %></a>
            <ul>
              <li><a href="<%=`/users/${currentUser._id}`%>">My Profile</a></li>
              <li><a href="/">My Orders</a></li>
            </ul>
          </li>
          <li><a href="/">cart</a></li>
          <li><a href="/users/logout">Log out</a></li>
          </li>

          <%} else {%>
          <li><a href="/users/login">Login</a></li>
          <% } %>
          <% } %>


        </ul>
